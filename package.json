{
  "name": "kit-stack",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "prisma": {
    "schema": "src/lib/db/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "bun db:generate && bun vite:dev",
    "build": "bun db:generate && bun vite:build",
    "vite:dev": "bun with-env vite dev",
    "vite:build": "bun with-env vite build",
    "db:generate": "bun with-env prisma generate",
    "db:push": "bun with-env prisma db push",
    "db:migrate": "bun with-env prisma migrate dev",
    "db:deploy": "bun with-env prisma migrate deploy",
    "db:studio": "bun with-env prisma studio",
    "lang:studio": "bun x @inlang/cli open editor",
    "preview": "bun with-env vite preview",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "with-env": "dotenv -e .env.local --",
    "env:sync": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "vercel:build": "~/.bun/bin/bun db:generate && ~/.bun/bin/bun db:deploy && ~/.bun/bin/bun vite:build",
    "storybook": "cross-env STORYBOOK=true storybook dev -p 6006 -s static",
    "storybook:build": "cross-env STORYBOOK=true storybook build -s static",
    "storybook:serve": "bun x serve storybook-static"
  },
  "devDependencies": {
    "@inlang/sdk-js": "^0.11.8",
    "@prisma/client": "5.2.0",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.4.0",
    "@storybook/svelte": "^7.4.0",
    "@storybook/sveltekit": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@stripe/stripe-js": "^2.1.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.24.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte": "^2.33.1",
    "postcss": "^8.4.29",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.0",
    "stripe": "^13.5.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-hero-icons": "^5.0.0",
    "svelte-stripe": "^0.0.22",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@felte/reporter-svelte": "^1.1.11",
    "@felte/validator-zod": "^1.0.17",
    "@fontsource/montserrat": "^5.0.8",
    "@fontsource/montserrat-alternates": "^5.0.8",
    "@supabase/auth-helpers-sveltekit": "^0.10.2",
    "@supabase/supabase-js": "^2.33.1",
    "@trpc/client": "^10.38.1",
    "@trpc/server": "^10.38.1",
    "cross-env": "^7.0.3",
    "daisyui": "^3.7.3",
    "dotenv-cli": "^7.3.0",
    "felte": "^1.2.12",
    "prisma": "^5.2.0",
    "superjson": "^1.13.1",
    "trpc-sveltekit": "^3.5.20",
    "zod": "^3.22.2"
  }
}
