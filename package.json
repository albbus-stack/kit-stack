{
  "name": "kit-stack",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "prisma": {
    "schema": "src/lib/db/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "pnpm db:generate && pnpm vite:dev",
    "build": "pnpm db:generate && pnpm vite:build",
    "vite:dev": "pnpm with-env vite dev",
    "vite:build": "pnpm with-env vite build",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push",
    "db:migrate": "pnpm with-env prisma migrate dev",
    "db:deploy": "pnpm with-env prisma migrate deploy",
    "db:studio": "pnpm with-env prisma studio",
    "lang:studio": "pnpm dlx @inlang/cli open editor",
    "preview": "pnpm with-env vite preview",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "with-env": "dotenv -e .env.local --",
    "env:sync": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "vercel:build": "pnpm db:generate && pnpm db:deploy && pnpm vite:build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@inlang/sdk-js": "^0.11.8",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.3.2",
    "@storybook/svelte": "^7.3.2",
    "@storybook/sveltekit": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@stripe/stripe-js": "^2.1.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.27",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.3.2",
    "stripe": "^13.1.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-hero-icons": "^5.0.0",
    "svelte-stripe": "^0.0.22",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "dependencies": {
    "@felte/reporter-svelte": "^1.1.10",
    "@felte/validator-zod": "^1.0.16",
    "@fontsource/montserrat": "^5.0.5",
    "@fontsource/montserrat-alternates": "^5.0.5",
    "@prisma/client": "5.0.0",
    "@supabase/auth-helpers-sveltekit": "^0.10.1",
    "@supabase/supabase-js": "^2.26.0",
    "@trpc/client": "^10.35.0",
    "@trpc/server": "^10.35.0",
    "daisyui": "^3.3.1",
    "dotenv-cli": "^7.2.1",
    "felte": "^1.2.11",
    "prisma": "^5.0.0",
    "superjson": "^1.13.1",
    "trpc-sveltekit": "^3.5.17",
    "zod": "^3.21.4"
  }
}
