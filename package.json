{
  "name": "kit-stack",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun db:generate && bun vite:dev",
    "build": "bun db:generate && bun vite:build",
    "vite:dev": "bun with-env vite dev",
    "vite:build": "bun with-env vite build",
    "db:generate": "bun with-env drizzle-kit generate:pg",
    "db:push": "bun with-env drizzle-kit push:pg",
    "db:studio": "bun with-env drizzle-kit studio",
    "lang:studio": "bun x @inlang/cli open editor",
    "preview": "bun with-env vite preview",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "with-env": "dotenv -e .env.local --",
    "env:sync": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "vercel:build": "~/.bun/bin/bun db:push && ~/.bun/bin/bun vite:build",
    "storybook": "cross-env STORYBOOK=true storybook dev -p 6006 -s static",
    "storybook:build": "cross-env STORYBOOK=true storybook build -s static",
    "storybook:serve": "bun x serve storybook-static"
  },
  "devDependencies": {
    "@inlang/sdk-js": "^0.11.8",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.3",
    "@storybook/svelte": "^7.5.3",
    "@storybook/sveltekit": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@stripe/stripe-js": "^2.1.11",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.27.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.35.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "stripe": "^14.3.0",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-hero-icons": "^5.0.0",
    "svelte-stripe": "^0.0.24",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@felte/reporter-svelte": "^1.1.11",
    "@felte/validator-zod": "^1.0.17",
    "@fontsource/montserrat": "^5.0.15",
    "@fontsource/montserrat-alternates": "^5.0.8",
    "@supabase/auth-helpers-sveltekit": "^0.10.7",
    "@supabase/supabase-js": "^2.38.4",
    "@trpc/client": "^10.43.1",
    "@trpc/server": "^10.43.1",
    "cross-env": "^7.0.3",
    "daisyui": "^3.9.4",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "drizzle-orm": "^0.28.6",
    "felte": "^1.2.12",
    "postgres": "^3.4.3",
    "superjson": "^2.2.0",
    "trpc-sveltekit": "^3.5.22",
    "zod": "^3.22.4"
  }
}
